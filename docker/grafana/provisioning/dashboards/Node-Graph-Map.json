          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\n  DISTINCT cd.node_id AS \"id\",\n  cd.long_name AS \"title\",\n  cd.short_name AS \"subtitle\",\n  cd.longitude,\n  cd.latitude\nFROM\n  node_details cd\n  LEFT JOIN (\n    SELECT\n      node_id\n    FROM\n      node_neighbors\n    UNION\n    SELECT\n      neighbor_id\n    FROM\n      node_neighbors\n  ) nn ON cd.node_id = nn.node_id\nWHERE\n  nn.node_id IS NOT NULL\n",
          "refId": "nodes",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
