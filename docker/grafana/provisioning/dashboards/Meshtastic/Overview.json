          "rawQuery": true,
          "rawSql": "SELECT\n  node_details.node_id AS \"Node ID\",\n  node_details.short_name AS \"Shortname\",\n  node_details.long_name AS \"Longname\",\n  node_details.hardware_model AS \"Hardware\",\n  device_metrics.battery_level AS \"Battery (%)\",\n  device_metrics.channel_utilization AS \"ChanUtil (%)\",\n  device_metrics.air_util_tx AS \"AirUtilTX (%)\"\nFROM node_details \nJOIN device_metrics ON node_details.node_id = device_metrics.node_id\nWHERE node_details.node_id <> '0' AND node_details.node_id IN($Nodes)\nORDER BY node_details.node_id;",
