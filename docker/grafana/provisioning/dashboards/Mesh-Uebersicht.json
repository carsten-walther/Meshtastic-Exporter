          "rawQuery": true,
          "rawSql": "SELECT\n  node_details.node_id AS \"Node ID\",\n  node_details.short_name AS \"Kurzname\",\n  node_details.long_name AS \"Langname\",\n  node_details.hardware_model AS \"Hardware\",\n  device_metrics.battery_level AS \"Batterie (%)\",\n  device_metrics.channel_utilization AS \"Kannalauslastung (%)\",\n  device_metrics.air_util_tx AS \"Air Utility TX (%)\",\n  node_details.updated_at AS \"Aktualisiert\"\nFROM\n  node_details\n  JOIN device_metrics ON node_details.node_id = device_metrics.node_id\n  JOIN mesh_packet_metrics ON node_details.node_id = mesh_packet_metrics.source_id\nWHERE\n  node_details.node_id <> '0'\n  AND node_details.node_id IN($nodeID)\nGROUP BY\n  node_details.node_id\nORDER BY\n  node_details.updated_at DESC;",
