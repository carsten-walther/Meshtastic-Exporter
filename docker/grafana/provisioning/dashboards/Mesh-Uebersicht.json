      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "P1809F7CD0C75ACF3"
          },
          "disableTextWrap": false,
          "editorMode": "code",
          "exemplar": false,
          "expr": "telemetry_app_air_util_tx{node_id=~\"$nodeID\"} > -100\nand\ndelta(telemetry_app_air_util_tx{node_id=~\"$nodeID\"}[1m]) != 0",
          "format": "time_series",
          "fullMetaSearch": false,
          "includeNullMetadata": false,
          "instant": false,
          "legendFormat": "{{long_name}}",
          "range": true,
          "rawQuery": true,
          "rawSql": "SELECT\n  device_metrics.time,\n  AVG(device_metrics.air_util_tx) as value,\n  CONCAT(\n    COALESCE(\n      node_details.short_name,\n      SUBSTRING(device_metrics.node_id, 1, 8)\n    ),\n    ' (',\n    node_details.long_name,\n    ')'\n  ) as metric\nFROM\n  device_metrics\n  LEFT JOIN node_details ON device_metrics.node_id = node_details.node_id\nWHERE\n  device_metrics.air_util_tx IS NOT NULL\n  AND $__timeFilter(device_metrics.time)\n  AND device_metrics.node_id IN ($nodeID)\nGROUP BY\n  device_metrics.time,\n  device_metrics.node_id,\n  node_details.short_name,\n  node_details.hardware_model\nORDER BY\n  device_metrics.time;",
